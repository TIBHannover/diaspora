<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../diaspora_to_csv/met_antibiogram_meta.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "http://kg.bacdive.dsmz.de/met_antibiogram_meta/{ID_met_antibiogram_meta}"; 
    rr:class bacdive:Met_antibiogram_meta ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_metabolite_antibiogram;
        rr:objectMap [ rml:reference "metabolite_antibiogram"]
    ] ;
    
    rr:predicateObjectMap [
        rr:predicate  bacdive:has_ID_DIN_antibiogram;
        rr:objectMap [ rml:reference "ID_DIN_antibiogram" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_ID_OXOID_antibiogram;
        rr:objectMap [ rml:reference "ID_OXOID_antibiogram" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_column_met_antibiogram;
    rr:objectMap [ rml:reference "column_met_antibiogram" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_Quantity_antibiogram;
    rr:objectMap [ rml:reference "Quantity_antibiogram" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_Unit_antibiogram;
    rr:objectMap [ rml:reference "Unit_antibiogram" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_chebi_ID;
    rr:objectMap [ rml:reference "chebi_ID" ]
    ] .



