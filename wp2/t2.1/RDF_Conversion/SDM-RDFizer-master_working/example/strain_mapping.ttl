      rml:reference "className";
	  rr:datatype xsd:string
      ]
    ] ;

 rr:predicateObjectMap [
    rr:predicate bd:orderName;
    rr:objectMap [
      rml:reference "ordo";
	  rr:datatype xsd:string
      ]
    ] ;
	
		 rr:predicateObjectMap [
    rr:predicate bd:familyName;
    rr:objectMap [
      rml:reference "family";
