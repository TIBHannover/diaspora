@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .

<TriplesMap1>	
	rml:logicalSource [ rml:source "C:/Gautam/TIB/SDM-RDFizer-master/example/motivating-example/dataset1_100recordsSample.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/RBP_RNA_PhysicalInteraction/{acc}_{enst}";
		rr:class iasis:RBP_RNA_PhysicalInteraction
	];
	rr:predicateObjectMap [
		rr:predicate iasis:omixcore;
		rr:objectMap [
			rml:reference "omixcore"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:interaction_involves_RBP;
		rr:objectMap [ 
			rr:parentTriplesMap <TriplesMap2> 
		]
	].		
<TriplesMap2>	
	rml:logicalSource [ rml:source "C:/Gautam/TIB/SDM-RDFizer-master/example/motivating-example/dataset1_100recordsSample.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Protein/{acc}";
		rr:class iasis:Protein
	];
	rr:predicateObjectMap [
		rr:predicate iasis:protein_isRelatedTo_exon;
		rr:objectMap [
			rr:parentTriplesMap <TriplesMap3>;
			rr:joinCondition [
				rr:child "enst";
				rr:parent "enst";
			];
		];
	].	
<TriplesMap3>	
	rml:logicalSource [ rml:source "C:/Gautam/TIB/SDM-RDFizer-master/example/motivating-example/dataset2_100recordsSample.csv";
						rml:referenceFormulation ql:CSV
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Exon/{ense}";
		rr:class iasis:Exon
	].