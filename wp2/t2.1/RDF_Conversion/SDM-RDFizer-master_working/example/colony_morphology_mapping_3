@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://example.com/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix bd: <http://www.tib.eu/bd/> .
@base <http://www.tib.eu/> .


<TripleMap1>
  a rr:TriplesMap;
    
   rml:logicalSource [
    rml:source <#DB_source>;
    rml:query "SELECT ID_colony_morphology, medium_name, medium_composition, colony_len, 
				colony_len_unit, colony_width
				,colony_width_unit,
				colony_shape,
				colony_color,
				incubation_period,
				hemolysis_type,
				hemolysis_ability, ID_strains FROM tib_bacdive.colony_morphology;";
    rml:referenceFormulation ql:CSV
  ];
  
 rr:subjectMap [
		rr:template "http://www.tib.eu/bd/{ID_strains}";
		rr:class bd:Bacteria
	] ;
	
	 rr:predicateObjectMap [
    rr:predicate bd:mediumName;
    rr:objectMap [
      rml:reference "medium_name";
	  rr:datatype xsd:string
      ]
    ] ;

rr:predicateObjectMap [
    rr:predicate bd:mediumComposition;
    rr:objectMap [
      rml:reference "medium_composition";
	  rr:datatype xsd:string
      ]
    ];
	
	rr:predicateObjectMap [
    rr:predicate bd:colonyLength;
    rr:objectMap [
      rml:reference "colony_len";
	  rr:datatype xsd:string
      ]
    ];

rr:predicateObjectMap [
    rr:predicate bd:colonyLengthUnit;
    rr:objectMap [
      rml:reference "colony_len_unit";
	  rr:datatype xsd:string
      ]
    ];	
	
	 rr:predicateObjectMap [
    rr:predicate bd:colonyWidth;
    rr:objectMap [
      rml:reference "colony_width";
	  rr:datatype xsd:boolean
      ]
    ];

rr:predicateObjectMap [
    rr:predicate bd:colonyWidthUnit;
    rr:objectMap [
      rml:reference "colony_width_unit";
	  rr:datatype xsd:string
      ]
    ];

rr:predicateObjectMap [
    rr:predicate bd:colonyShape;
    rr:objectMap [
      rml:reference "colony_shape";
	  rr:datatype xsd:string
      ]
    ];

	 rr:predicateObjectMap [
    rr:predicate bd:colonyColor;
    rr:objectMap [
      rml:reference "colony_color";
	  rr:datatype xsd:boolean
      ]
    ];

rr:predicateObjectMap [
    rr:predicate bd:incubationPeriod;
    rr:objectMap [
      rml:reference "incubation_period";
	  rr:datatype xsd:string
      ]
    ];

rr:predicateObjectMap [
    rr:predicate bd:hemolysisType;
    rr:objectMap [
      rml:reference "hemolysis_type";
	  rr:datatype xsd:string
      ]
    ];

rr:predicateObjectMap [
    rr:predicate bd:hemolysisAbility;
    rr:objectMap [
      rml:reference "hemolysis_ability";
	  rr:datatype xsd:string
      ]
    ] .
	