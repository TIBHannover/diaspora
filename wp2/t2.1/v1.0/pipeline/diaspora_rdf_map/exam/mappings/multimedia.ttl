@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix matonto: <http://matonto.org/ontology/matonto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.com/ns/>.
@prefix bacdive: <http://kg.bacdive.dsmz.de/>.



<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../diaspora_to_csv/multimedia.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "http://kg.bacdive.dsmz.de/multimedia/{ID_multimedia}"; 
    rr:class bacdive:Multimedia ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_file_name;
        rr:objectMap [ rml:reference "file_name"]
    ] ;
    
    rr:predicateObjectMap [
        rr:predicate  bacdive:has_file_path;
        rr:objectMap [ rml:reference "file_path" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_file;
        rr:objectMap [ rml:reference "file" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_full_file_path;
    rr:objectMap [ rml:reference "full_file_path" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_FSI_URL;
    rr:objectMap [ rml:reference "FSI_URL" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_media_type;
    rr:objectMap [ rml:reference "media_type" ]
    ] ;



    rr:predicateObjectMap [
    rr:predicate  bacdive:has_IPR;
    rr:objectMap [ rml:reference "IPR" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_license_type;
    rr:objectMap [ rml:reference "license_type" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_caption;
    rr:objectMap [ rml:reference "caption" ]
    ] ;


    rr:predicateObjectMap [
    rr:predicate  bacdive:has_created;
    rr:objectMap [ rml:reference "created" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_creator;
    rr:objectMap [ rml:reference "creator" ]
    ] ;
    
    rr:predicateObjectMap [
    rr:predicate  bacdive:has_capture_equipment;
    rr:objectMap [ rml:reference "capture_equipment" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_reference;
    rr:objectMap [ rr:template "http://kg.bacdive.dsmz.de/reference/{ID_reference}" ]
    ] ;
    
   

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_strain;
    rr:objectMap [ rr:template "http://kg.bacdive.dsmz.de/strains/{ID_strains}" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_manual_annotation;
    rr:objectMap [ rml:reference "manual_annotation" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_FK_tbl_media_micro;
    rr:objectMap [ rml:reference "FK_tbl_media_micro" ]
    ] ;


    rr:predicateObjectMap [
    rr:predicate  bacdive:has_timestamp_micro;
    rr:objectMap [ rml:reference "timestamp_micro" ]
    ] ;


    rr:predicateObjectMap [
    rr:predicate  bacdive:has_last_change;
    rr:objectMap [ rml:reference "last_change" ]
    ] .

