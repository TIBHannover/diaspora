<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../diaspora_to_csv/spore_formation.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "http://kg.bacdive.dsmz.de/spore_formation/{ID_spore_formation}"; 
    rr:class bacdive:Spore_formation ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_ability;
        rr:objectMap [ rml:reference "ability"]
    ] ;
    
    rr:predicateObjectMap [
        rr:predicate  bacdive:has_type;
        rr:objectMap [ rml:reference "type" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_description;
        rr:objectMap [ rml:reference "description" ]
    ] .
