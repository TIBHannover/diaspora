@prefix bd: <http://www.tib.eu/bd/> .
@base <http://www.tib.eu/> .


<TripleMap1>
  a rr:TriplesMap;
    
  rml:logicalSource [
    rml:source "C:/Gautam/Gautam_Working/diaspora_Github/wp2/t2.1/RDF_Conversion/SDM-RDFizer-master_working/example/files/strains_filtered.csv";
    rml:referenceFormulation ql:CSV
  ];
  
  rr:subjectMap [
		rr:template "http://www.tib.eu/bd/{ID_strains}";
		rr:class bd:Bacteria
	] ;
	
