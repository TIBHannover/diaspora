<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../diaspora_to_csv/risk_assessment.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "http://kg.bacdive.dsmz.de/risk_assessment/{ID_strains}"; 
    rr:class bacdive:Risk_assessment ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_biosafety_level;
        rr:objectMap [ rml:reference "biosafety_level"]
    ] ;
    
    rr:predicateObjectMap [
        rr:predicate  bacdive:has_biosafety_level_comment;
        rr:objectMap [ rml:reference "biosafety_level_comment" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_pathogenicity_human;
        rr:objectMap [ rml:reference "pathogenicity_human" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_pathogenicity_animal;
    rr:objectMap [ rml:reference "pathogenicity_animal" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_pathogenicity_plant;
    rr:objectMap [ rml:reference "pathogenicity_plant" ]
    ] .

