<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../diaspora_to_csv/annotation_user.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "http://kg.bacdive.dsmz.de/annotation_user/{ID_annotation_user}"; 
    rr:class bacdive:Annotation_user ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_user_id;
        rr:objectMap [ rml:reference "user_id"]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_pw;
        rr:objectMap [ rml:reference "pw"]
    ] ;

    
    rr:predicateObjectMap [
        rr:predicate  bacdive:has_name;
        rr:objectMap [ rml:reference "name"]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_surname;
        rr:objectMap [ rml:reference "surname"]
    ] ;

     rr:predicateObjectMap [
        rr:predicate  bacdive:has_mail;
        rr:objectMap [ rml:reference "mail"]
    ] ;

      rr:predicateObjectMap [
        rr:predicate  bacdive:has_mail_verified;
        rr:objectMap [ rml:reference "mail_verified"]
    ] ;

        rr:predicateObjectMap [
        rr:predicate  bacdive:has_ID_approval_rights;
        rr:objectMap [ rml:reference "approval_rights"]
    ] ;

        rr:predicateObjectMap [
        rr:predicate  bacdive:has_Created;
        rr:objectMap [ rml:reference "Created"]
    ] ;

        rr:predicateObjectMap [
        rr:predicate  bacdive:has_Last_Login;
        rr:objectMap [ rml:reference "Last_Login"]
    ] ;

            rr:predicateObjectMap [
        rr:predicate  bacdive:has_Last_Changed;
        rr:objectMap [ rml:reference "Last_Changed"]
    ] ;

        rr:predicateObjectMap [
        rr:predicate  bacdive:has_session_ID;
        rr:objectMap [ rml:reference "session_ID"]
    ] .