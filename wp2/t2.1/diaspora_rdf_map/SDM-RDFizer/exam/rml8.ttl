@prefix bacdive: <http://kg.bacdive.dsmz.de/>.




<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../diaspora_to_csv/met_antibiotica.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "http://kg.bacdive.dsmz.de/cell_morphology/{ID_strains}"; 
    rr:class bacdive:Met_antibiotica ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_metabolite_antib;
        rr:objectMap [ rml:reference "metabolite_antib"]
    ] ;
    
    rr:predicateObjectMap [
        rr:predicate  bacdive:has_MET_group_ID;
        rr:objectMap [ rml:reference "MET_group_ID" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_is_antibiotic;
        rr:objectMap [ rml:reference "is_antibiotic" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_ab_sensitive;
    rr:objectMap [ rml:reference "ab_sensitive" ]
    ] ;

    # rr:predicateObjectMap [
    # rr:predicate  bacdive:has_ab_sensitive_conc;
    # rr:objectMap [ rml:reference "ab_sensitive_conc" ]
    # ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_rgStart_sens;
    rr:objectMap [ rml:reference "rgStart_sens" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_rgEnd_sens;
    rr:objectMap [ rml:reference "rgEnd_sens" ]
    ] ;

    # rr:predicateObjectMap [
    # rr:predicate  bacdive:has_ab_sensitive_unit;
    # rr:objectMap [ rml:reference "ab_sensitive_unit" ]
    # ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_ab_intermediate;
    rr:objectMap [ rml:reference "ab_intermediate" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_ab_intermediate_conc;
    rr:objectMap [ rml:reference "ab_intermediate_conc" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_rgStart_int;
    rr:objectMap [ rml:reference "rgStart_int" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_rgEnd_int;
    rr:objectMap [ rml:reference "rgEnd_int" ]
    ] .
    
    
    

