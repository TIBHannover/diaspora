@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix matonto: <http://matonto.org/ontology/matonto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.com/ns/>.
@prefix bacdive: <http://kg.bacdive.dsmz.de/>.



<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../diaspora_to_csv/section.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "http://kg.bacdive.dsmz.de/section/{ID_section}"; 
    rr:class bacdive:Metabolites ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_section;
        rr:objectMap [ rml:reference "section"]
    ] ;

       rr:predicateObjectMap [
        rr:predicate  bacdive:has_section_web;
        rr:objectMap [ rml:reference "section_web"]
    ] ;

       rr:predicateObjectMap [
        rr:predicate  bacdive:has_help_text;
        rr:objectMap [ rml:reference "help_text"]
    ] ;

       rr:predicateObjectMap [
        rr:predicate  bacdive:has_position;
        rr:objectMap [ rml:reference "position"]
    ] ;

       rr:predicateObjectMap [
        rr:predicate  bacdive:has_last_change;
        rr:objectMap [ rml:reference "last_change"]
    ] .
