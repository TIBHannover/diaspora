@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix matonto: <http://matonto.org/ontology/matonto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.com/ns/>.
@prefix bacdive: <http://kg.bacdive.dsmz.de/>.
@prefix qudt: <http://qudt.org/2.1/schema/qudt#> .




<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../diaspora_to_csv/culture_temp.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "http://kg.bacdive.dsmz.de/culture_temp/{ID_culture_temp}"; 
    rr:class bacdive:Culture_temp ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_temp;
        rr:objectMap [ rml:reference "temp"]
    ] ;
    

    rr:predicateObjectMap [
    rr:predicate  qudt:unit ;
    rr:objectMap[rr:template "Celcius";
        rr:termType rr:Literal;
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_rgStart;
        rr:objectMap [ rml:reference "rgStart" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_rgEnd;
        rr:objectMap [ rml:reference "rgEnd" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_test_type;
    rr:objectMap [ rml:reference "test_type" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_ability;
    rr:objectMap [ rml:reference "ability" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_temperature_range;
    rr:objectMap [ rml:reference "temperature_range" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_reference;
    rr:objectMap [ rr:template "http://kg.bacdive.dsmz.de/reference/{ID_reference}" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_strain;
    rr:objectMap [ rr:template "http://kg.bacdive.dsmz.de/strains/{ID_strains}" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_manual_annotation;
    rr:objectMap [ rml:reference "manual_annotation" ]
    ] ;


    rr:predicateObjectMap [
    rr:predicate  bacdive:has_last_change;
    rr:objectMap [ rml:reference "last_change" ]
    ] .


