@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix matonto: <http://matonto.org/ontology/matonto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.com/ns/>.
@prefix bacdive: <http://kg.bacdive.dsmz.de/>.



<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../diaspora_to_csv/culture_condition.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "http://kg.bacdive.dsmz.de/culture_condition/{ID_culture_condition}"; 
    rr:class bacdive:Culture_condition ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_medium_name;
        rr:objectMap [ rml:reference "medium_name"]
    ] ;
    
    rr:predicateObjectMap [
        rr:predicate  bacdive:has_medium_composition;
        rr:objectMap [ rml:reference "medium_composition" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_oxygen_tol;
        rr:objectMap [ rml:reference "oxygen_tol" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_pH_max;
    rr:objectMap [ rml:reference "pH_max" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_pH_opt;
    rr:objectMap [ rml:reference "pH_opt" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_pH_min;
    rr:objectMap [ rml:reference "pH_min" ]
    ] ;



    rr:predicateObjectMap [
    rr:predicate  bacdive:has_temp_max;
    rr:objectMap [ rml:reference "temp_max" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_temp_opt;
    rr:objectMap [ rml:reference "temp_opt" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_temp_min;
    rr:objectMap [ rml:reference "temp_min" ]
    ] ;


    rr:predicateObjectMap [
    rr:predicate  bacdive:has_temperature_range;
    rr:objectMap [ rml:reference "temperature_range" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_media_link;
    rr:objectMap [ rml:reference "media_link" ]
    ] ;
    
    rr:predicateObjectMap [
    rr:predicate  bacdive:has_media_link_name;
    rr:objectMap [ rml:reference "media_link_name" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_reference;
    rr:objectMap [ rr:template "http://kg.bacdive.dsmz.de/reference/{ID_reference}" ]
    ] ;
    
   

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_strain;
    rr:objectMap [ rr:template "http://kg.bacdive.dsmz.de/strains/{ID_strains}" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_manual_annotation;
    rr:objectMap [ rml:reference "manual_annotation" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_FK_tbl_media_micro;
    rr:objectMap [ rml:reference "FK_tbl_media_micro" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_last_change;
    rr:objectMap [ rml:reference "last_change" ]
    ] .

