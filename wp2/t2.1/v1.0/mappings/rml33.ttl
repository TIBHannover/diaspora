@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix matonto: <http://matonto.org/ontology/matonto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.com/ns/>.
@prefix bacdive: <http://kg.bacdive.dsmz.de/>.




<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../diaspora_to_csv/culture_collection.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "http://kg.bacdive.dsmz.de/culture_collection/{ID_culture_collection}"; 
    rr:class bacdive:Culture_collection ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_acronym_col;
        rr:objectMap [ rml:reference "Acronym_col"]
    ] ;
    
    rr:predicateObjectMap [
        rr:predicate  bacdive:has_acronym_col_num;
        rr:objectMap [ rml:reference "Acronym_col_num" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_url_general;
        rr:objectMap [ rml:reference "URL_general" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_url_str_num;
    rr:objectMap [ rml:reference "URL_str_num" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_culture_collection;
    rr:objectMap [ rml:reference "Culture_Collection" ]
    ] .

    
    

