@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix matonto: <http://matonto.org/ontology/matonto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.com/ns/>.
@prefix bacdive: <http://kg.bacdive.dsmz.de/>.




<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../diaspora_to_csv/origin.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "http://kg.bacdive.dsmz.de/origin/{ID_origin}"; 
    rr:class bacdive:Origin ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_geo_loc_name;
        rr:objectMap [ rml:reference "geo_loc_name"]
    ] ;
    
    rr:predicateObjectMap [
        rr:predicate  bacdive:has_country;
        rr:objectMap [ rml:reference "country" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_continent;
        rr:objectMap [ rml:reference "continent" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_latitude;
    rr:objectMap [ rml:reference "latitude" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_longitude;
    rr:objectMap [ rml:reference "longitude" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_sample_date;
    rr:objectMap [ rml:reference "sample_date" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_sample_type;
    rr:objectMap [ rml:reference "sample_type" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_Host_Species;
    rr:objectMap [ rml:reference "Host_Species" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_enrichment_cult_name;
    rr:objectMap [ rml:reference "enrichment_cult_name" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_enrichment_cult_composition;
    rr:objectMap [ rml:reference "enrichment_cult_composition" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_enrichment_cult_duration;
    rr:objectMap [ rml:reference "enrichment_cult_duration" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_enrichment_cult_temp;
    rr:objectMap [ rml:reference "enrichment_cult_temp" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_procedure_origin;
    rr:objectMap [ rml:reference "procedure_origin" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_isolation_date;
    rr:objectMap [ rml:reference "isolation_date" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_id_reference;
    rr:objectMap [ rr:template "http://kg.bacdive.dsmz.de/reference/{ID_reference}" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_ID_strains;
    rr:objectMap [ rr:template "http://kg.bacdive.dsmz.de/strains/{ID_strains}" ]
    ] .





    
    
    
    
    

