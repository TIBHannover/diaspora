@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix matonto: <http://matonto.org/ontology/matonto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.com/ns/>.
@prefix bacdive: <http://kg.bacdive.dsmz.de/>.



<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../diaspora_to_csv/annotation_core.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "http://kg.bacdive.dsmz.de/annotation_core/{ID_annotation_core}"; 
    rr:class bacdive:Annotation_core ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_action;
        rr:objectMap [ rml:reference "action"]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_table_name;
        rr:objectMap [ rml:reference "table_name"]
    ] ;

    
    rr:predicateObjectMap [
        rr:predicate  bacdive:has_source_table_ID;
        rr:objectMap [ rml:reference "source_table_ID"]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_field_name;
        rr:objectMap [ rml:reference "field_name"]
    ] ;

     rr:predicateObjectMap [
        rr:predicate  bacdive:has_old_value;
        rr:objectMap [ rml:reference "old_value"]
    ] ;

        rr:predicateObjectMap [
        rr:predicate  bacdive:has_new_value;
        rr:objectMap [ rml:reference "new_value"]
    ] ;

        rr:predicateObjectMap [
        rr:predicate  bacdive:has_occurred;
        rr:objectMap [ rml:reference "occurred"]
    ] ;

        rr:predicateObjectMap [
        rr:predicate  bacdive:has_ID_annotation_event;
        rr:objectMap [ rml:reference "ID_annotation_event"]
    ] .

