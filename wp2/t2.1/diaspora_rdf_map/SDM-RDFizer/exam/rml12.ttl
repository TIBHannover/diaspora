@prefix bacdive: <http://kg.bacdive.dsmz.de/>.




<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../diaspora_to_csv/oxygen_tolerance.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "http://kg.bacdive.dsmz.de/oxygen_tolerance/{ID_strains}"; 
    rr:class bacdive:Oxygen_tolerance ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_oxygen_tol;
        rr:objectMap [ rml:reference "oxygen_tol"]
    ] .



    
    
    
    
    

