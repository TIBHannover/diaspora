<<TriplesMap1>
  a rr:TriplesMap;

  rml:logicalSource [
    rml:source <#DB_source>;
    rr:sqlVersion rr:SQL2008;
 	rml:query """SELECT * FROM tib_bacdive.multicellular_morphology; """ .
    rr:tableName "tib_bacdive.multicellular_morphology";
];
	rr:subjectMap [
		rr:template "http://www.tib.eu/bd/{ID_multicellular_morphology}";
		rr:class bd:Bacteria
	] ;
	
	 rr:predicateObjectMap [
    rr:predicate bd:motility;
    rr:objectMap [
      rml:reference "ability";
	  rr:datatype xsd:boolean
      ]

    ].	

