@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://example.com/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix bd: <http://www.tib.eu/bd/> .
@base <http://www.tib.eu/> .


<TripleMap1>
  a rr:TriplesMap;
    
  rml:logicalSource [
    rml:source "C:/Gautam/Gautam_Working/diaspora_Github/wp2/t2.1/RDF_Conversion/SDM-RDFizer-master_working/example/files/cell_morphology_filtered.csv";
    rml:referenceFormulation ql:CSV
  ];
  
  rr:subjectMap [
		rr:template "http://www.tib.eu/bd/{ID_strains}";
		rr:class bd:Bacteria
	] ;
	
	 rr:predicateObjectMap [
    rr:predicate bd:cellLength_aaaaabbc;
    rr:objectMap [
      rml:reference "cell_len";
	  rr:datatype xsd:string
      ]
    ] ;

rr:predicateObjectMap [
    rr:predicate bd:cellLengthUnit;
    rr:objectMap [
      rml:reference "cell_len_unit";
	  rr:datatype xsd:string
      ]
    ];
	
	rr:predicateObjectMap [
    rr:predicate bd:widthOfCell;
    rr:objectMap [
      rml:reference "cell_width";
	  rr:datatype xsd:string
      ]
    ];

rr:predicateObjectMap [
    rr:predicate bd:cellShape;
    rr:objectMap [
      rml:reference "cell_shape";
	  rr:datatype xsd:string
      ]
    ];	
	
	 rr:predicateObjectMap [
    rr:predicate bd:motility;
    rr:objectMap [
      rml:reference "motility";
	  rr:datatype xsd:boolean
      ]
    ];

rr:predicateObjectMap [
    rr:predicate bd:flagellumArrangement;
    rr:objectMap [
      rml:reference "flagellum_arrangement";
	  rr:datatype xsd:string
      ]
    ];

rr:predicateObjectMap [
    rr:predicate bd:gramStain;
    rr:objectMap [
      rml:reference "gram_stain";
	  rr:datatype xsd:string
      ]
    ];
	
	rr:predicateObjectMap [ 
    rr:predicate bd:hasPh; 
    rr:objectMap [
      rr:parentTriplesMap <TripleMap2>;
      rr:joinCondition [
        rr:child "ph";
        rr:parent "Bacteria";
      ];
    ];
  ].

<TripleMap2>
  a rr:TriplesMap;
    
  rml:logicalSource [
    rml:source "C:/Gautam/Gautam_Working/diaspora_Github/wp2/t2.1/RDF_Conversion/SDM-RDFizer-master_working/example/files/culture_ph_filtered.csv";
    rml:referenceFormulation ql:CSV
  ];
  rr:subjectMap [
    rr:template "http://www.tib.eu/bd/{ID_strains}";
    rr:object bd:hasPh
  ];
 rr:predicateObjectMap [
    rr:predicate bd:ph;
    rr:objectMap [
      rml:reference "pH";
	  rr:datatype xsd:string
      ]
    ] .  



	
	
	
	
	