@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix matonto: <http://matonto.org/ontology/matonto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.com/ns/>.
@prefix bacdive: <http://kg.bacdive.dsmz.de/>.




<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../diaspora_to_csv/is_cat3.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "http://kg.bacdive.dsmz.de/is_cat3/{PK_IS_CAT3}"; 
    rr:class bacdive:Is_cat3 ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_cat3;
        rr:objectMap [ rml:reference "Cat3"]
    ] ;
    
    rr:predicateObjectMap [
        rr:predicate  bacdive:has_cat3_description;
        rr:objectMap [ rml:reference "Cat3_description" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_cat3_synonyms;
        rr:objectMap [ rml:reference "Cat3_synonyms" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_fk_cat2;
    rr:objectMap [ rml:reference "FK_Cat2" ]
    ] ;

    rr:predicateObjectMap [
    rr:predicate  bacdive:has_chart_id_cat3;
    rr:objectMap [ rml:reference "Chart_ID_Cat3" ]
    ] .

 
  



