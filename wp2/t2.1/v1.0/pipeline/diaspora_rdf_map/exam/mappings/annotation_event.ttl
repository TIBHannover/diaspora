@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix matonto: <http://matonto.org/ontology/matonto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.com/ns/>.
@prefix bacdive: <http://kg.bacdive.dsmz.de/>.



<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../diaspora_to_csv/annotation_event.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "http://kg.bacdive.dsmz.de/annotation_event/{ID_annotation_event}"; 
    rr:class bacdive:Annotation_event ];

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_approved;
        rr:objectMap [ rml:reference "approved"]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_ID_approval_user;
        rr:objectMap [ rml:reference "ID_approval_user"]
    ] ;

    
    rr:predicateObjectMap [
        rr:predicate  bacdive:has_ID_annotation_user;
        rr:objectMap [ rml:reference "ID_annotation_user"]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  bacdive:has_created;
        rr:objectMap [ rml:reference "Created"]
    ] ;

     rr:predicateObjectMap [
        rr:predicate  bacdive:has_last_changed;
        rr:objectMap [ rml:reference "Last_Changed"]
    ] .
